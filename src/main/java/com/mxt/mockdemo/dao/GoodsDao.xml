<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mxt.mockdemo.dao.GoodsDao">

    <select id="getGoodsById" resultType="com.mxt.mockdemo.entity.Goods">
        select * from goods where  id = #{id};
    </select>
    <select id="getGoodsByOwner" resultType="com.mxt.mockdemo.entity.Goods">
          select * from goods where  owner = #{owner};
    </select>


</mapper>